{% extends 'base.jinja2' %}


{% block content_body %}


<form action="{{url('sparta_uploadcsv', kwargs={'activity_id': activity_id})}}" method="POST" enctype="multipart/form-data">
{{ csrf_input }}

<div align="center" class="demo-card-square mdl-card mdl-shadow--2dp">
  <div align="center" class="mdl-card__title mdl-card--expand">
    <h2 class="mdl-card__title-text">Envie o CSV com os Alunos e notas</h2>
  </div>
  <div align="center" class="mdl-card__supporting-text">
  {{ form.csv_file }}
  </div>
  <div align="center" class="mdl-card__actions mdl-card--border">
    <button class="mdl-button mdl-button--colored mdl-js-button mdl-js-ripple-effect" type="submit">Enviar</button>
  </div>
</div>
</form>


{% endblock %}
